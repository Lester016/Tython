# mag sulat o mag awtput
sulat("Kumusta, Mundo!\n")

# mga baryabol
x = 1
y = 2

# mga aritmetika
sulat(x + y)
sulat(x - y)
sulat(x * y)
sulat(x / y)

# magtukoy ng gawain
tukuyin magbawas_ng_lima(x):
	ibalik x - 5
# isulat ang ibinalik nang gawaing tinukoy
sulat(magbawas_ng_lima(12))

# mag-input
