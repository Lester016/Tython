# mga lohikal na opereytor
sulat(1 < 5 at  8 < 10)
sulat(5 == 5 o 2 == 4)
sulat(hindi(3 < 5 at 6 < 10))

# mag-ulit o loop
prutas = ["mansanas", "saging", "ubas"] # listahan
ulitin x ang prutas:
  sulat(x)